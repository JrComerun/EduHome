
@{
    ViewData["Title"] = "Course";
}
<!-- Banner Area Start -->
@await Component.InvokeAsync("SectionTitle", new { title = "Courses" })
<!-- Banner Area End -->
<!-- Course Start -->
<div class="course-area pt-150 pb-150">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <div class="course-title">
                    <h3>search courses</h3>
                </div>
                <div class="course-form">
                    <form id="search" action="#">
                        <input id="search-input-course" type="text" placeholder="Search..." name="search" />
                    </form>
                </div>
            </div>
        </div>
        <div class="row" id="courses-list">

        </div>
        @await Component.InvokeAsync("Course", null)
    </div>
</div>
<!-- Course End -->
