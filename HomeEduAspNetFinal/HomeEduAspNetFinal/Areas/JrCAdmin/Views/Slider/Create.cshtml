@model HomeSlider
@{
    ViewData["Title"] = "Create";
}
<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Create Category</h4>
            <form class="forms-sample" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for="@Model.Photo"></label>
                    <input class="form-control" accept="image/*" asp-for="@Model.Photo">
                    <span asp-validation-for="@Model.Photo" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="@Model.Photo"></label>
                    <input class="form-control"  asp-for="@Model.Photo">
                    <span asp-validation-for="@Model.Photo" class="text-danger"></span>
                </div>
                <button type="submit" class="btn btn-primary mr-2">Submit</button>
                <a asp-action="Index" class="btn btn-secondary">Go Back</a>
            </form>
        </div>
    </div>
</div>
<h2>Create</h2>
@section Scripts{ 
<partial name="_ScriptsPartial" />
}
